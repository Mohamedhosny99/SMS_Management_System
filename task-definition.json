{
  "family": "sms-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "sms_web_app",
      "image": "mayarfahmy1/sms_web_app:latest",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::692859933235:role/ecsTaskExecutionRole"
}
